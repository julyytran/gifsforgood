<%= form_for(@gif) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= label_tag :tags %>
  <%= text_field_tag :tags %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit "add new gif!" %>
<% end %>
