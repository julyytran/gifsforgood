<%= form_for(@gif) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :tags %>
  <%= f.text_field :tags %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.submit "add new gif!" %>
<% end %>
